{
  "name": "ai-ide-backend",
  "version": "1.0.0",
  "description": "AI-IDE Backend - Node.js + Express API server with Google Drive integration and Docker container management",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "validate": "npm run lint && npm run format:check",
    "build": "echo 'No build step required for Node.js'",
    "docker:build": "docker build -t ai-ide-backend .",
    "docker:run": "docker run -p 3001:3001 ai-ide-backend"
  },
  "keywords": [
    "ide",
    "web-ide",
    "docker",
    "google-drive",
    "monaco-editor",
    "code-execution",
    "lsp"
  ],
  "author": "AI-IDE Team",
  "license": "MIT",
  "dependencies": {
    "@huggingface/inference": "^2.8.1",
    "bcryptjs": "^3.0.2",
    "chokidar": "^4.0.3",
    "compression": "^1.8.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.4.0",
    "googleapis": "^161.0.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "mongoose": "^8.18.3",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.8.1",
    "tar-stream": "^3.1.7",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
